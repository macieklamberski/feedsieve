{
  "name": "feedsieve",
  "description": "Classify and deduplicate feed items into inserts and updates.",
  "repository": {
    "type": "git",
    "url": "https://github.com/macieklamberski/feedsieve.git"
  },
  "bugs": {
    "url": "https://github.com/macieklamberski/feedsieve/issues"
  },
  "license": "MIT",
  "author": "Maciej Lamberski",
  "sideEffects": false,
  "keywords": [
    "rss",
    "atom",
    "feed",
    "classify",
    "deduplicate",
    "diff",
    "sync"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "lefthook install",
    "build": "tsdown src/index.ts --format cjs,esm --dts --clean --unbundle --no-fixed-extension"
  },
  "dependencies": {
    "feedcanon": "^1.3.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "kvalita": "1.9.0",
    "tsdown": "^0.20.1"
  }
}
